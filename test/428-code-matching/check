function errorIfDoesNotHave {
  sed -n "/[0-9]: void Main.$2/,/^  [0-9]:/p" $1 | grep $3 > /dev/null || exit 1
}

function errorIfHas {
  sed -n "/[0-9]: void Main.$2/,/^  [0-9]:/p" $1 | grep $3 > /dev/null
  if [ $? = 0 ]; then
    exit 1
  fi
}

errorIfDoesNotHave $2 "\$opt\$ArrayStoreNeedsTypeCheck" "pAputObject"
errorIfHas $2 "\$opt\$ArrayStoreNoNeedTypeCheck" "pAputObject"
